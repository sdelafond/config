$purge_build_deps="never";
$build_arch_all = 1; 
$build_source = 1;
$apt_upgrade = 0;
$apt_distupgrade = 0;

$lintian_opts = ['-I'];

# autopkgtests
$run_autopkgtest = 1;
$chroot_mode = 'autopkgtest';
$autopkgtest_virt_server = 'schroot';
$autopkgtest_virt_server_options = ['%r-%a-sbuild'];
$autopkgtest_root_args = '';

# don't remove this, Perl needs it:
1;

