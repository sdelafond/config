(require 'selection-push)
(bind-keys global-keymap
	   "M-z" '(push-current-selection)
	   "S-M-Insert" '(dump-selections)
	   "S-M-Delete" '(clear-selections))

(require 'keyboard-paste)
(bind-keys global-keymap 
	   "S-Insert" 'keyboard-paste
	   "M-Delete" 'clear-selection)
